<?xml version="1.0" encoding="UTF-8"?>
<rss xmlns:nb="https://www.newsbreak.com/" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:content="http://purl.org/rss/1.0/modules/content/" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:media="http://search.yahoo.com/mrss/" xmlns:dcterms="http://purl.org/dc/terms/" version="2.0">
    <channel>
        <atom:link href="{{ feed_url }}" rel="self" type="application/rss+xml" />
        <title>Straight Arrow News. Unbiased. Straight Facts.</title>
        <description>Straight Arrow News. Unbiased. Straight Facts.</description>
        <link>https://san.com</link>
        {% for item in items %}
        <item>
            <title><![CDATA[{{ item.title }}]]></title>
            <link>{{ item.link }}</link>
            <pubDate>{{ item.pubdate }}</pubDate>
            <dc:creator><![CDATA[{{ item.author }}]]></dc:creator>
            <description><![CDATA[{{ item.description }}]]></description>
            <content:encoded><![CDATA[{{ item.content | safe }}]]></content:encoded>
            <media:thumbnail
                url="{{ item.thumbnail_url }}"
                width="1280" height="720" />
            <nb:scripts><![CDATA[<script async src="https://www.googletagmanager.com/gtag/js?id=G-T4EW2F5KD2"></script>
                        <script>
                        window.dataLayer = window.dataLayer || [];
                        function gtag(){dataLayer.push(arguments);}
                        gtag('js', new Date());
                        gtag('config', 'G-T4EW2F5KD2');
                        </script>]]>
                    </nb:scripts>
        </item>
        {% endfor %}
    </channel>
</rss>
