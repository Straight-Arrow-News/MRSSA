<rss xmlns:atom="http://www.w3.org/2005/Atom" xmlns:media="http://search.yahoo.com/mrss/"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:content="http://purl.org/rss/1.0/modules/content/" version="2.0">
    <channel>
        <atom:link href="{{ feed_url }}" rel="self" type="application/rss+xml" />
        <title>Straight Arrow News. Unbiased. Straight Facts.</title>
        <description>Straight Arrow News. Unbiased. Straight Facts.</description>
        <link>https://san.com</link>
        {% for item in items %}
        <item>
            <title><![CDATA[ {{ item.title | safe }} ]]></title>
            <guid isPermaLink="false">{{ item.guid }}</guid>
            <link>{{ item.link }}</link>
            <pubDate>{{ item.pubdate }}</pubDate>
            <media:title><![CDATA[ {{ item.title }} ]]></media:title>
            <description><![CDATA[ {{ item.description }} ]]></description>
            <author><![CDATA[ {{ item.author }} ]]></author>
            <content:encoded><![CDATA[ {{ item.content | safe }} ]]></content:encoded>
            <dcterms:valid>start={{ item.valid_start }};scheme=W3C-DTF</dcterms:valid>
            <media:thumbnail url="{{ item.thumbnail_url }}" width="1280" height="720" />
            <media:player url="{{ item.player_url }}" width="1280" height="720" />
            <media:keywords><![CDATA[ text {{ item.keywords }} ]]></media:keywords>
        </item>
        {% endfor %}
    </channel>
</rss>
